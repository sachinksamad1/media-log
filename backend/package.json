{
  "name": "media-log-backend",
  "version": "1.0.0",
  "description": "Media Log Backend",
  "main": "dist/server.js",
  "packageManager": "pnpm@10.28.0",
  "scripts": {
    "seed": "tsx seed/seed.ts",
    "seed:users": "tsx seed/seed-users.ts",
    "start": "node dist/server.js",
    "build": "tsc && tsc-alias",
    "dev": "tsx watch src/server.ts",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "typecheck": "tsc --noEmit",
    "check": "pnpm lint && pnpm typecheck",
    "format": "prettier --write src",
    "format:check": "prettier --check src"
  },
  "keywords": [
    "mediaLog",
    "backend",
    "firebase",
    "typescript",
    "nodejs"
  ],
  "author": "Sachin Kumar Samad",
  "license": "MIT",
  "type": "module",
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^25.0.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-promise": "^7.2.1",
    "globals": "^17.0.0",
    "prettier": "^3.7.4",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.52.0"
  },
  "dependencies": {
    "@types/nodemailer": "^7.0.5",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "firebase-admin": "^13.6.0",
    "helmet": "^8.1.0",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.12",
    "sharp": "^0.34.5",
    "uuid": "^13.0.0",
    "zod": "^4.3.4"
  }
}
