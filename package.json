{
  "name": "media-log-monorepo",
  "packageManager": "pnpm@10.28.1",
  "private": true,
  "scripts": {
    "build:web": "cd web && pnpm build",
    "build:backend": "cd backend && pnpm build",
    "build": "pnpm build:web && pnpm build:backend",
    "deploy": "firebase deploy",
    "deploy:web": "firebase deploy --only hosting",
    "deploy:backend": "firebase deploy --only functions",
    "clean": "rd /s /q web/dist backend/dist"
  },
  "engines": {
    "node": "20"
  }
}